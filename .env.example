# Neo4j Connection Settings
NEO4J_URI="neo4j+s://your-instance-id.databases.neo4j.io"
NEO4J_USER="your_username"
NEO4J_PASSWORD="your_password"
NEO4J_DATABASE="neo4j"
# Neo4j Database Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password
NEO4J_DATABASE=neo4j

# Allow unsafe memory Cypher queries (without security node)
# Set to "true" to enable, any other value or absence to disable
NEO4J_UNSAFE_MEMORY_CYPHERS=false

# LLM API Configuration
# Specify which provider to use (default is 'openai' if not specified)
LLM_API_PROVIDER=openai
# Model to use with the specified provider
LLM_API_MODEL=gpt-4

# Provider-specific API keys
# Only the key for the selected provider is required
OPENAI_API_KEY=your_openai_key_here
#ANTHROPIC_API_KEY=your_anthropic_key_here
#MISTRAL_API_KEY=your_mistral_key_here
#GEMINI_API_KEY=your_gemini_key_here
#GROQ_API_KEY=your_groq_key_here
#PERPLEXITY_API_KEY=your_perplexity_key_here
#OPENROUTER_API_KEY=your_openrouter_key_here
#AI21_API_KEY=your_ai21_key_here
#COHERE_API_KEY=your_cohere_key_here

# AWS Bedrock configuration (if using AWS Bedrock provider)
#AWS_REGION_NAME=us-east-1
#AWS_ACCESS_KEY_ID=your_aws_access_key
#AWS_SECRET_ACCESS_KEY=your_aws_secret_key
